var
	ct: parameter;
	x_9, hcool_9, hrest_9: clock;


automaton Rod_9h

synclabs: cool_9, rest_9;

loc l0_9: while True wait {}
		 when -ct + x_9 >= 0 sync cool_9 do {hcool_9' = 0} goto l1_9;

loc l1_9: while True wait {}
		 when True sync rest_9 do {x_9' = 0, hrest_9' = 0} goto l0_9;

end


var init: region;

init:= loc[Rod_9h] = l0_9
		 & x_9 = 0 & hcool_9 = 0 & hrest_9 = 0
;