var
	ct: parameter;
	x_4, hcool_4, hrest_4: clock;


automaton Rod_4h

synclabs: cool_4, rest_4;

loc l0_4: while True wait {}
		 when -ct + x_4 >= 0 sync cool_4 do {hcool_4' = 0} goto l1_4;

loc l1_4: while True wait {}
		 when True sync rest_4 do {x_4' = 0, hrest_4' = 0} goto l0_4;

end


var init: region;

init:= loc[Rod_4h] = l0_4
		 & x_4 = 0 & hcool_4 = 0 & hrest_4 = 0
;