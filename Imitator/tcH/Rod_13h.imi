var
	ct: parameter;
	x_13, hcool_13, hrest_13: clock;


automaton Rod_13h

synclabs: cool_13, rest_13;

loc l0_13: while True wait {}
		 when -ct + x_13 >= 0 sync cool_13 do {hcool_13' = 0} goto l1_13;

loc l1_13: while True wait {}
		 when True sync rest_13 do {x_13' = 0, hrest_13' = 0} goto l0_13;

end


var init: region;

init:= loc[Rod_13h] = l0_13
		 & x_13 = 0 & hcool_13 = 0 & hrest_13 = 0
;