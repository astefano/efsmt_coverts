var
	ct: parameter;
	x_1, hcool_1, hrest_1: clock;


automaton Rod_1h

synclabs: cool_1, rest_1;

loc l0_1: while True wait {}
		 when -ct + x_1 >= 0 sync cool_1 do {hcool_1' = 0} goto l1_1;

loc l1_1: while True wait {}
		 when True sync rest_1 do {x_1' = 0, hrest_1' = 0} goto l0_1;

end


var init: region;

init:= loc[Rod_1h] = l0_1
		 & x_1 = 0 & hcool_1 = 0 & hrest_1 = 0
;