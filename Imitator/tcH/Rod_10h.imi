var
	ct: parameter;
	x_10, hcool_10, hrest_10: clock;


automaton Rod_10h

synclabs: cool_10, rest_10;

loc l0_10: while True wait {}
		 when -ct + x_10 >= 0 sync cool_10 do {hcool_10' = 0} goto l1_10;

loc l1_10: while True wait {}
		 when True sync rest_10 do {x_10' = 0, hrest_10' = 0} goto l0_10;

end


var init: region;

init:= loc[Rod_10h] = l0_10
		 & x_10 = 0 & hcool_10 = 0 & hrest_10 = 0
;