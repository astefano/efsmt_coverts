var
	ct: parameter;
	x_15, hcool_15, hrest_15: clock;


automaton Rod_15h

synclabs: cool_15, rest_15;

loc l0_15: while True wait {}
		 when -ct + x_15 >= 0 sync cool_15 do {hcool_15' = 0} goto l1_15;

loc l1_15: while True wait {}
		 when True sync rest_15 do {x_15' = 0, hrest_15' = 0} goto l0_15;

end


var init: region;

init:= loc[Rod_15h] = l0_15
		 & x_15 = 0 & hcool_15 = 0 & hrest_15 = 0
;