var
	ct: parameter;
	x_25, hcool_25, hrest_25: clock;


automaton Rod_25h

synclabs: cool_25, rest_25;

loc l0_25: while True wait {}
		 when -ct + x_25 >= 0 sync cool_25 do {hcool_25' = 0} goto l1_25;

loc l1_25: while True wait {}
		 when True sync rest_25 do {x_25' = 0, hrest_25' = 0} goto l0_25;

end


var init: region;

init:= loc[Rod_25h] = l0_25
		 & x_25 = 0 & hcool_25 = 0 & hrest_25 = 0
;