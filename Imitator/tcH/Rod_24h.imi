var
	ct: parameter;
	x_24, hcool_24, hrest_24: clock;


automaton Rod_24h

synclabs: cool_24, rest_24;

loc l0_24: while True wait {}
		 when -ct + x_24 >= 0 sync cool_24 do {hcool_24' = 0} goto l1_24;

loc l1_24: while True wait {}
		 when True sync rest_24 do {x_24' = 0, hrest_24' = 0} goto l0_24;

end


var init: region;

init:= loc[Rod_24h] = l0_24
		 & x_24 = 0 & hcool_24 = 0 & hrest_24 = 0
;