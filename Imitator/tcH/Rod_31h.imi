var
	ct: parameter;
	x_31, hcool_31, hrest_31: clock;


automaton Rod_31h

synclabs: cool_31, rest_31;

loc l0_31: while True wait {}
		 when -ct + x_31 >= 0 sync cool_31 do {hcool_31' = 0} goto l1_31;

loc l1_31: while True wait {}
		 when True sync rest_31 do {x_31' = 0, hrest_31' = 0} goto l0_31;

end


var init: region;

init:= loc[Rod_31h] = l0_31
		 & x_31 = 0 & hcool_31 = 0 & hrest_31 = 0
;