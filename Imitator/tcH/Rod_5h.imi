var
	ct: parameter;
	x_5, hcool_5, hrest_5: clock;


automaton Rod_5h

synclabs: cool_5, rest_5;

loc l0_5: while True wait {}
		 when -ct + x_5 >= 0 sync cool_5 do {hcool_5' = 0} goto l1_5;

loc l1_5: while True wait {}
		 when True sync rest_5 do {x_5' = 0, hrest_5' = 0} goto l0_5;

end


var init: region;

init:= loc[Rod_5h] = l0_5
		 & x_5 = 0 & hcool_5 = 0 & hrest_5 = 0
;