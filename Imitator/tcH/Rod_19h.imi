var
	ct: parameter;
	x_19, hcool_19, hrest_19: clock;


automaton Rod_19h

synclabs: cool_19, rest_19;

loc l0_19: while True wait {}
		 when -ct + x_19 >= 0 sync cool_19 do {hcool_19' = 0} goto l1_19;

loc l1_19: while True wait {}
		 when True sync rest_19 do {x_19' = 0, hrest_19' = 0} goto l0_19;

end


var init: region;

init:= loc[Rod_19h] = l0_19
		 & x_19 = 0 & hcool_19 = 0 & hrest_19 = 0
;