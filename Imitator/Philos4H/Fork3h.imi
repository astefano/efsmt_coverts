var
		htakefork3, hreleasefork3: clock;


automaton Fork3h

synclabs: takefork3, releasefork3;

loc f30: while True wait {}
		 when True sync takefork3 do {htakefork3' = 0} goto f31;

loc f31: while True wait {}
		 when True sync releasefork3 do {hreleasefork3' = 0} goto f30;

end


var init: region;

init:= loc[Fork3h] = f30
		 & htakefork3 = 0 & hreleasefork3 = 0
;