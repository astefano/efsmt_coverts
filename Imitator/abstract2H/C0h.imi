var
	q: parameter;
	x, ha, hc0: clock;


automaton C0h

synclabs: a, c0;

loc c00: while -x >= -7 wait {}
		 when -q + x >= 0 sync a do {ha' = 0} goto c01;

loc c01: while -x >= -10 wait {}
		 when True sync c0 do {x' = 0, hc0' = 0} goto c00;

end


var init: region;

init:= loc[C0h] = c00
		 & x = 0 & ha = 0 & hc0 = 0
;