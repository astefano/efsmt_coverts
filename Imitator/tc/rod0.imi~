var
	ct: parameter;
	x_0: clock;

automaton Rod_0

synclabs: ini_0, cool_0, rest_0;

loc l0_0: while True wait {}
	when True sync ini_0 do {x_0'=ct} goto l1_0;

loc l1_0: while True wait {}
        when x_0 >= ct sync cool_0 do {} goto l2_0;

loc l2_0: while True wait {}
        when True sync rest_0 do {x_0'=0} goto l1_0;

end

var init: region;

init:=	loc[Rod_0] = l0_0  &	
	x_0 = 0 
(* ct = 900N *)
;


