var
	p, q: parameter;
	x, y, ha: clock;


automaton C8h

synclabs: a;

loc c0: while p - x >= 0 wait {}
		 when -p + x >= 0 sync a do {ha' = 0} goto c1;

loc c1: while -y >= -10 wait {}
		 when y >= 0 sync a do {y' = 0, ha' = 0} goto c2;

loc c2: while q - x >= 0 wait {}
		 when x >= 0 sync a do {x' = 0, ha' = 0} goto c0;

end


var init: region;

init:= loc[C8h] = c0
		 & x = 0 & y = 0 & ha = 0
;