var
	ct: parameter;
	x_28, hcool_28, hrest_28: clock;


automaton Rod_28h

synclabs: cool_28, rest_28;

loc l0_28: while True wait {}
		 when -ct + x_28 >= 0 sync cool_28 do {hcool_28' = 0} goto l1_28;

loc l1_28: while True wait {}
		 when True sync rest_28 do {x_28' = 0, hrest_28' = 0} goto l0_28;

end


var init: region;

init:= loc[Rod_28h] = l0_28
		 & x_28 = 0 & hcool_28 = 0 & hrest_28 = 0
;