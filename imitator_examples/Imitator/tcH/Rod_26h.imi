var
	ct: parameter;
	x_26, hcool_26, hrest_26: clock;


automaton Rod_26h

synclabs: cool_26, rest_26;

loc l0_26: while True wait {}
		 when -ct + x_26 >= 0 sync cool_26 do {hcool_26' = 0} goto l1_26;

loc l1_26: while True wait {}
		 when True sync rest_26 do {x_26' = 0, hrest_26' = 0} goto l0_26;

end


var init: region;

init:= loc[Rod_26h] = l0_26
		 & x_26 = 0 & hcool_26 = 0 & hrest_26 = 0
;