var
	ct: parameter;
	x_8, hcool_8, hrest_8: clock;


automaton Rod_8h

synclabs: cool_8, rest_8;

loc l0_8: while True wait {}
		 when -ct + x_8 >= 0 sync cool_8 do {hcool_8' = 0} goto l1_8;

loc l1_8: while True wait {}
		 when True sync rest_8 do {x_8' = 0, hrest_8' = 0} goto l0_8;

end


var init: region;

init:= loc[Rod_8h] = l0_8
		 & x_8 = 0 & hcool_8 = 0 & hrest_8 = 0
;