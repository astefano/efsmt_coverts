var
	ct: parameter;
	x_23, hcool_23, hrest_23: clock;


automaton Rod_23h

synclabs: cool_23, rest_23;

loc l0_23: while True wait {}
		 when -ct + x_23 >= 0 sync cool_23 do {hcool_23' = 0} goto l1_23;

loc l1_23: while True wait {}
		 when True sync rest_23 do {x_23' = 0, hrest_23' = 0} goto l0_23;

end


var init: region;

init:= loc[Rod_23h] = l0_23
		 & x_23 = 0 & hcool_23 = 0 & hrest_23 = 0
;