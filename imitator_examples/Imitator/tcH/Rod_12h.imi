var
	ct: parameter;
	x_12, hcool_12, hrest_12: clock;


automaton Rod_12h

synclabs: cool_12, rest_12;

loc l0_12: while True wait {}
		 when -ct + x_12 >= 0 sync cool_12 do {hcool_12' = 0} goto l1_12;

loc l1_12: while True wait {}
		 when True sync rest_12 do {x_12' = 0, hrest_12' = 0} goto l0_12;

end


var init: region;

init:= loc[Rod_12h] = l0_12
		 & x_12 = 0 & hcool_12 = 0 & hrest_12 = 0
;