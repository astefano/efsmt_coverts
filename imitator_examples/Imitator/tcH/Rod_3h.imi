var
	ct: parameter;
	x_3, hcool_3, hrest_3: clock;


automaton Rod_3h

synclabs: cool_3, rest_3;

loc l0_3: while True wait {}
		 when -ct + x_3 >= 0 sync cool_3 do {hcool_3' = 0} goto l1_3;

loc l1_3: while True wait {}
		 when True sync rest_3 do {x_3' = 0, hrest_3' = 0} goto l0_3;

end


var init: region;

init:= loc[Rod_3h] = l0_3
		 & x_3 = 0 & hcool_3 = 0 & hrest_3 = 0
;