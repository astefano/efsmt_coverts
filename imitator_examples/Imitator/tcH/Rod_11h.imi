var
	ct: parameter;
	x_11, hcool_11, hrest_11: clock;


automaton Rod_11h

synclabs: cool_11, rest_11;

loc l0_11: while True wait {}
		 when -ct + x_11 >= 0 sync cool_11 do {hcool_11' = 0} goto l1_11;

loc l1_11: while True wait {}
		 when True sync rest_11 do {x_11' = 0, hrest_11' = 0} goto l0_11;

end


var init: region;

init:= loc[Rod_11h] = l0_11
		 & x_11 = 0 & hcool_11 = 0 & hrest_11 = 0
;