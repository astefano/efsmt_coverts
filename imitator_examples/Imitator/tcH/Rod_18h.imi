var
	ct: parameter;
	x_18, hcool_18, hrest_18: clock;


automaton Rod_18h

synclabs: cool_18, rest_18;

loc l0_18: while True wait {}
		 when -ct + x_18 >= 0 sync cool_18 do {hcool_18' = 0} goto l1_18;

loc l1_18: while True wait {}
		 when True sync rest_18 do {x_18' = 0, hrest_18' = 0} goto l0_18;

end


var init: region;

init:= loc[Rod_18h] = l0_18
		 & x_18 = 0 & hcool_18 = 0 & hrest_18 = 0
;