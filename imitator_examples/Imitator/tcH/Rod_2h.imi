var
	ct: parameter;
	x_2, hcool_2, hrest_2: clock;


automaton Rod_2h

synclabs: cool_2, rest_2;

loc l0_2: while True wait {}
		 when -ct + x_2 >= 0 sync cool_2 do {hcool_2' = 0} goto l1_2;

loc l1_2: while True wait {}
		 when True sync rest_2 do {x_2' = 0, hrest_2' = 0} goto l0_2;

end


var init: region;

init:= loc[Rod_2h] = l0_2
		 & x_2 = 0 & hcool_2 = 0 & hrest_2 = 0
;